  },
  "bugs": {
    "url": "https://github.com/cirruslabs/cirrus-ci-docs/issues"
  },
  "homepage": "https://cirrus-ci.com",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@improbable-eng/grpc-web": "^0.14.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.12.1",
    "@types/google-protobuf": "^3.15.2",
    "ansi_up": "^5.0.0",
    "babel-plugin-relay": "^10.1.2",
    "classnames": "^2.2.6",
    "clipboard-copy": "^4.0.1",
    "google-protobuf": "^3.17.3",
    "graphiql": "^1.4.0",
    "graphql": "^15.4.0",
    "graphql-relay": "^0.6.0",
    "js-base64": "^3.6.0",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "^16.8.4",
    "react-ace": "^9.2.1",
    "react-dom": "^16.8.4",
    "react-helmet": "^6.1.0",
    "react-markdown": "^5.0.3",
    "react-relay": "^10.1.2",
    "react-router-dom": "^5.2.0",
    "react-stripe-elements": "^6.1.2",
    "react-vis": "^1.11.7",
    "recharts": "^1.8.5",
    "recoil": "^0.1.2",
    "ts-protoc-gen": "^0.15.0",
    "xterm": "^4.12.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.28",
    "@types/react": "^16.8.0",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-relay": "^7.0.17",
    "@types/react-router-dom": "^5.1.6",
    "@types/relay-runtime": "^10.1.6",
    "get-graphql-schema": "^2.1.2",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-scripts": "^3.4.3",
    "react-test-renderer": "^17.0.1",
    "relay-compiler": "^10.1.2",
    "relay-compiler-language-typescript": "^13.0.6",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-untyped": "TSC_COMPILE_ON_ERROR=\"true\" react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --write .",
    "prettier-check": "prettier --check .",
    "relay": "relay-compiler --verbose --watchman false --src ./src --schema schema.graphql --language typescript",
    "sync-schema": "get-graphql-schema https://api.cirrus-ci.com/schema.json | sed -e 's/Long/Int/g' > schema.graphql && prettier --write schema.graphql",
    "bootstrap": "yarn sync-schema && yarn relay"
  },
  "babelMacros": {
    "relay": {}
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
