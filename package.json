{
  "name": "cirrus-ci-web",
  "description": "The source that powers https://cirrus-ci.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cirruslabs/cirrus-ci-web.git"
  },
  "bugs": {
    "url": "https://github.com/cirruslabs/cirrus-ci-docs/issues"
  },
  "homepage": "https://cirrus-ci.com",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "~4.4.3",
    "@material-ui/icons": "~4.4.3",
    "ansi_up": "^4.0.4",
    "babel-plugin-relay": "*",
    "classnames": "^2.2.6",
    "graphql": "^14.5.8",
    "graphql-relay": "^0.6.0",
    "js-base64": "^2.5.1",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.12.0",
    "react-ace": "^7.0.4",
    "react-dom": "^16.12.0",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.2.2",
    "react-relay": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^5.1.0",
    "react-vis": "^1.11.7",
    "recharts": "~1.8.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-relay": "^6.0.0",
    "@types/react-router-dom": "^5.1.3",
    "@types/relay-runtime": "^6.0.5",
    "get-graphql-schema": "^2.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.0",
    "react-scripts": "^3.2.0",
    "relay-compiler": "^6.0.0",
    "relay-compiler-language-typescript": "^7.0.0",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-untyped": "TSC_COMPILE_ON_ERROR=\"true\" react-scripts start",
    "build": "react-scripts build",
    "relay": "npx relay-compiler --verbose --watchman false --src ./src --schema schema.graphql --language typescript",
    "sync-schema": "get-graphql-schema https://api.cirrus-ci.com/schema.json | sed -e 's/Long/Int/g' > schema.graphql",
    "test": "react-scripts test --env=jsdom",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prettier": "prettier --write \"{src,public}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "prettier-check": "prettier --check  \"{src,public}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\""
  },
  "babelMacros": {
    "relay": {}
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.1%",
    "not dead",
    "not op_mini all"
  ]
}
