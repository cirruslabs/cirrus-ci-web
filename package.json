{
  "name": "cirrus-ci-web",
  "description": "The source that powers https://cirrus-ci.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/cirruslabs/cirrus-ci-web.git"
  },
  "bugs": {
    "url": "https://github.com/cirruslabs/cirrus-ci-docs/issues"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "ansi_up": "*",
    "classnames": "*",
    "graphql": "*",
    "graphql-relay": "*",
    "js-base64": "*",
    "pluralize": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-loadable": "*",
    "react-markdown": "*",
    "react-relay": "*",
    "react-router-dom": "*",
    "react-stripe-elements": "*",
    "recharts": "*"
  },
  "devDependencies": {
    "babel-preset-react-app": "*",
    "flow-bin": "*",
    "get-graphql-schema": "*",
    "js-beautify": "*",
    "relay-compiler": "*",
    "relay-modern-scripts": "*"
  },
  "scripts": {
    "start": "PORT=3333 react-scripts start",
    "build": "react-scripts build",
    "flow": "flow",
    "relay": "relay-compiler --verbose --watchman false --src ./src --schema schema.graphql",
    "sync-schema": "get-graphql-schema https://api.cirrus-ci.com/schema.json > schema.graphql",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
